<h1>Listing addicional_informations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @addicional_informations.each do |addicional_information| %>
      <tr>
        <td><%= link_to 'Show', addicional_information %></td>
        <td><%= link_to 'Edit', edit_addicional_information_path(addicional_information) %></td>
        <td><%= link_to 'Destroy', addicional_information, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Addicional information', new_addicional_information_path %>
